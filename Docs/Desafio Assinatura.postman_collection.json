							"// Fixed the ReferenceError by using pm.response.json()",
							"const { access_token } = pm.response.json();",
							"",
							"const partes = access_token.split(\"|\");",
							"if(access_token){",
							"    pm.collectionVariables.set('token', partes[1]);",
							"}",
							"",
							"console.log(pm.collectionVariables.get('token'));"
